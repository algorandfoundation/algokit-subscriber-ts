{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts"],
  "exclude": ["src/**/*.spec.ts", "tests/**/*.*"],
  "entryPointStrategy": "expand",
  "out": "docs/code",
  "plugin": ["typedoc-plugin-markdown"],
  "theme": "default",
  "cleanOutputDir": true,
  "githubPages": false,
  "readme": "none",
  "entryDocument": "README.md",
  "gitRevision": "main"
}
