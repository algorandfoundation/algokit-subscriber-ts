{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts", "src/types/*.ts"],
  "exclude": ["src/**/*.spec.ts", "tests/**/*.*", "examples/**/*.*"],
  "entryPointStrategy": "expand",
  "projectDocuments": ["docs/README.md", "docs/subscriber.md", "docs/subscriptions.md", "docs/v3-migration.md"],
  "outputs": [{ "name": "html", "path": "docs/_html" }],
  "plugin": ["typedoc-plugin-markdown"],
  "theme": "default",
  "cleanOutputDir": true,
  "githubPages": false,
  "readme": "docs/README.md",
  "gitRevision": "main",
  "tsconfig": "tsconfig.build.json",
  "highlightLanguages": ["typescript", "python", "json"]
}
