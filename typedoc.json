{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src/index.ts",
    "src/types/*.ts"
  ],
  "plugin": [
    "typedoc-plugin-markdown",
    "typedoc-plugin-frontmatter",
    "@larkiny/algo-docs-sync/dist/plugins/custom-frontmatter.mjs"
  ],
  "readmeFrontmatter": {
    "title": "Overview",
    "sidebar": {
      "order": 0
    }
  },
  "readme": "none",
  "excludePrivate": true,
  "excludeInternal": true,
  "hideGenerator": true,
  "hideBreadcrumbs": true,
  "tsconfig": "tsconfig.build.json",
  "out": "docs-build/api",
  "githubPages": false,
  "exclude": [
    "src/**/*.spec.ts",
    "tests/**/*.*",
    "examples/**/*.*"
  ]
}
